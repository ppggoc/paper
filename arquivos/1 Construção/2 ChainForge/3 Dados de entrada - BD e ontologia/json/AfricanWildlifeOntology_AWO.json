{
    "ontology": {
        "name": "African Wildlife Ontology (AWO)",
        "description": "Ontologia didática sobre fauna e flora africanas, criada por Maria Keet e Frances Gillis-Webber para demonstrar modelagem OWL e consultas OBDA. Representa relações ecológicas entre animais, plantas e habitats, com exemplos do Parque Nacional Kruger.",
        "domain": [
            "fauna",
            "flora",
            "ecologia",
            "habitats",
            "África do Sul"
        ]
    },
    "classes": {
        "Animal": {
            "description": "Ser vivo pertencente ao reino Animalia; disjunto de Plant.",
            "disjointWith": [
                "Plant"
            ],
            "subclasses": {
                "Carnivore": {
                    "description": "Animal que se alimenta exclusivamente de outros animais.",
                    "subclasses": [
                        "HypoCarnivore",
                        "MesoCarnivore",
                        "ObligateCarnivore"
                    ]
                },
                "Herbivore": {
                    "description": "Animal que se alimenta exclusivamente de plantas.",
                    "examples": [
                        "Elephant",
                        "Impala"
                    ]
                },
                "Omnivore": {
                    "description": "Animal que se alimenta tanto de plantas quanto de outros animais.",
                    "examples": [
                        "Warthog"
                    ]
                },
                "Lion": {
                    "description": "Espécie específica de carnívoro."
                },
                "Giraffe": {
                    "description": "Espécie específica de herbívoro."
                },
                "RockDassie": {
                    "description": "Pequeno mamífero africano (Procavia capensis)."
                },
                "Elephant": {
                    "description": "Grande herbívoro terrestre africano."
                },
                "Impala": {
                    "description": "Antílope africano, herbívoro, comum no Kruger Park."
                },
                "Warthog": {
                    "description": "Javali-africano onívoro."
                },
                "AnimalCollective": {
                    "description": "Agrupamentos ou coletivos de animais.",
                    "examples": [
                        "Herd",
                        "Swarm",
                        "Colony",
                        "Nest",
                        "Troop",
                        "Rhumba",
                        "Bask",
                        "Zeal",
                        "Kettle",
                        "Cloud"
                    ]
                }
            }
        },
        "Plant": {
            "description": "Organismo do reino Plantae; disjunto de Animal.",
            "disjointWith": [
                "Animal"
            ],
            "subclasses": {
                "Tree": {
                    "description": "Plantas lenhosas de grande porte."
                },
                "Grass": {
                    "description": "Plantas herbáceas, comuns em savanas."
                },
                "Palm": {
                    "description": "Plantas do tipo palmeira."
                },
                "TastyPlant": {
                    "description": "Plantas consideradas alimento por herbívoros."
                },
                "PlantParts": {
                    "description": "Partes estruturais de uma planta.",
                    "subclasses": {
                        "Root": {
                            "description": "Raízes de uma planta.",
                            "subclasses": [
                                "Carrot",
                                "Parsnip"
                            ]
                        },
                        "Stem": {
                            "description": "Caule principal da planta."
                        },
                        "Branch": {
                            "description": "Ramo ou galho."
                        },
                        "Leaf": {
                            "description": "Folha da planta."
                        },
                        "Twig": {
                            "description": "Pequeno galho ou broto."
                        },
                        "Phloem": {
                            "description": "Tecido condutor de seiva elaborada."
                        },
                        "Xylem": {
                            "description": "Tecido condutor de seiva bruta."
                        },
                        "FruitingBody": {
                            "description": "Estruturas reprodutivas com frutos ou sementes.",
                            "examples": [
                                "Berry",
                                "Apple"
                            ]
                        }
                    }
                }
            }
        },
        "Habitat": {
            "description": "Local ocupado por pelo menos duas entidades biológicas (animais e plantas)."
        },
        "Role": {
            "description": "Função ecológica desempenhada por um indivíduo.",
            "subclasses": [
                "Predator",
                "Prey"
            ]
        },
        "NationalPark": {
            "description": "Área de conservação que abriga espécies e habitats naturais.",
            "subclasses": [
                "SouthAfricaNationalPark"
            ]
        },
        "DeprecatedClass": {
            "description": "Classes obsoletas ou não mais utilizadas.",
            "examples": [
                "Palmtree"
            ]
        },
        "Distribution": {
            "description": "Representa a distribuição geográfica das espécies."
        }
    },
    "objectProperties": {
        "eats": {
            "domain": "Animal",
            "range": "Animal or Plant",
            "inverseOf": "eaten-by",
            "description": "Relação alimentar entre predador e presa."
        },
        "eaten-by": {
            "domain": "Animal or Plant",
            "range": "Animal",
            "inverseOf": "eats",
            "description": "Indica que um ser é consumido por outro."
        },
        "inhabits": {
            "domain": "Animal or Plant",
            "range": "Habitat",
            "description": "Indica o habitat em que o indivíduo vive."
        },
        "has-member": {
            "domain": "AnimalCollective",
            "range": "Animal",
            "description": "Associa um coletivo aos indivíduos que o compõem."
        },
        "has-part": {
            "domain": "Plant or Animal",
            "range": "PlantParts or AnimalPart",
            "inverseOf": "is-part-of",
            "description": "Define uma relação de composição parte-todo."
        },
        "is-part-of": {
            "domain": "PlantParts or AnimalPart",
            "range": "Plant or Animal",
            "inverseOf": "has-part",
            "description": "Indica que algo é parte de um todo."
        },
        "is-proper-part-of": {
            "description": "Versão mais restrita de 'is-part-of', usada para evitar reflexividade."
        },
        "inheres-in": {
            "domain": "Role",
            "range": "Individual",
            "description": "Relaciona um papel ecológico a um indivíduo específico."
        },
        "tagged-in": {
            "domain": "Animal",
            "range": "Location or Study",
            "description": "Representa o local ou contexto em que o animal foi marcado."
        },
        "has-taxon": {
            "domain": "Animal",
            "range": "Taxon",
            "description": "PROPRIEDADE DE DADO DE UM ANIMAL, DO TIPO STRING."
        }
    },
    "individuals": {
        "impala1": {
            "type": "Impala",
            "description": "Instância de um antílope Impala, herbívoro africano."
        },
        "lion1": {
            "type": "Lion",
            "description": "Instância de um leão, carnívoro africano, predador natural do impala."
        }
    }
}